<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>add_to_cart</servlet-name>
        <servlet-class>gamefreak_servlets.add_to_cart</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>add_to_wishlist</servlet-name>
        <servlet-class>gamefreak_servlets.add_to_wishlist</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>delete_cart_item</servlet-name>
        <servlet-class>gamefreak_servlets.delete_cart_item</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>delete_wishlist_item</servlet-name>
        <servlet-class>gamefreak_servlets.delete_wishlist_item</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>delete_address</servlet-name>
        <servlet-class>gamefreak_servlets.delete_address</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewServlet</servlet-name>
        <servlet-class>beans.NewServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>order_placement</servlet-name>
        <servlet-class>gamefreak_servlets.order_placement</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>cancel_delivery</servlet-name>
        <servlet-class>gamefreak_servlets.cancel_delivery</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>refund</servlet-name>
        <servlet-class>gamefreak_servlets.refund</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>return_order</servlet-name>
        <servlet-class>gamefreak_servlets.return_order</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>add_to_cart</servlet-name>
        <url-pattern>/add_to_cart</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>add_to_wishlist</servlet-name>
        <url-pattern>/add_to_wishlist</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>delete_cart_item</servlet-name>
        <url-pattern>/delete_cart_item</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>delete_wishlist_item</servlet-name>
        <url-pattern>/delete_wishlist_item</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>delete_address</servlet-name>
        <url-pattern>/delete_address</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>order_placement</servlet-name>
        <url-pattern>/order_placement</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>cancel_delivery</servlet-name>
        <url-pattern>/cancel_delivery</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>refund</servlet-name>
        <url-pattern>/refund</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>return_order</servlet-name>
        <url-pattern>/return_order</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
